{
  "name": "lmpeek",
  "version": "1.0.0",
  "description": "A library for analyzing LLM internals in web browsers.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "homepage": "https://github.com/amanvir/lmpeek",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amanvir/lmpeek.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "llm",
    "gpt-2",
    "onnx",
    "mechanistic-interpretability"
  ],
  "author": "amanvir_",
  "license": "MIT",
  "dependencies": {
    "@lenml/tokenizer-gpt2": "1.1.2",
    "@xenova/transformers": "2.17.2",
    "idb-keyval": "6.2.2",
    "onnxruntime-web": "1.22.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}